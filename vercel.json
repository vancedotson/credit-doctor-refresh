{
  "rewrites": [
    {
      "source": "/((?!api|_next|_static|_vercel|[\\w-]+\\.\\w+).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/service-worker.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
